// MÃ¶bius Function Computation
// Time Complexity: O(n log log n)
void computeMobius(int n, vector<int>& mu) {
    vector<int> is_prime(n + 1, 1);
    mu[0] = 0;
    mu[1] = 1;

    for (int i = 2; i <= n; ++i) {
        if (is_prime[i]) {
            for (int j = i; j <= n; j += i) {
                mu[j] *= -1;
                is_prime[j] = 0;
            }

            for (int j = i * i; j <= n; j += i * i) {
                mu[j] = 0; 
            }
        }
    }
}
